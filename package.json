{
  "name": "react-temporal",
  "version": "0.1.0",
  "description": "Headless Datetime Picker for React",
  "main": "dist/useTemporal.js",
  "sideEffects": false,
  "scripts": {
    "dev": "esbuild src/useTemporal.ts --outfile=./dist/useTemporal.js --watch --sourcemap",
    "build": "rimraf dist && esbuild src/useTemporal.ts --outfile=./dist/useTemporal.js --minify"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/garand/react-temporal.git"
  },
  "keywords": [
    "react",
    "calendar",
    "datepicker",
    "datetime",
    "headless"
  ],
  "author": "Anthony Garand",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/garand/react-temporal/issues"
  },
  "homepage": "https://github.com/garand/react-temporal#readme",
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "esbuild": "^0.12.9",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.3.1",
    "react": "^17.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.2"
  }
}

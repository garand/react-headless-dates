{
  "name": "react-temporal",
  "version": "0.1.3",
  "description": "Headless Datetime Picker for React",
  "author": "Anthony Garand",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/garand/react-temporal.git"
  },
  "bugs": {
    "url": "https://github.com/garand/react-temporal/issues"
  },
  "homepage": "https://github.com/garand/react-temporal#readme",
  "keywords": [
    "react",
    "calendar",
    "datepicker",
    "datetime",
    "headless"
  ],
  "main": "./dist/useTemporal.js",
  "types": "./dist/useTemporal.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "rimraf dist && concurrently npm:dev:*",
    "dev:src": "esbuild src/useTemporal.ts --outfile=./dist/useTemporal.js --watch --sourcemap",
    "dev:types": "tsc --watch",
    "build": "rimraf dist && concurrently npm:build:*",
    "build:src": "esbuild src/useTemporal.ts --outfile=./dist/useTemporal.js --minify",
    "build:types": "tsc"
  },
  "devDependencies": {
    "@types/react": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "concurrently": "^6.2.0",
    "esbuild": "^0.12.9",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.3.1",
    "react": "^17.0.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.4"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  }
}
